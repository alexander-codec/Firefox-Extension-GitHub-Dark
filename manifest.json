{
  "description": "Ativar modo escuro no Github",
  "manifest_version": 2,
  "name": "Github Dark",
  "author": "Johnendz",
  "version": "1.2",

  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "css":["style.css"],
      "js": ["jquery.min.js", "project-board.js"],
	  "run_at": "document_start"
    }
  ],
  
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  }
}